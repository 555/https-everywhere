<!--
	Fully covered hosts in *fefe.de:

		- blog
		- dl
		- ptrace
		- www


	^fefe.de doesn't exist.

-->
<ruleset name="fefe.de">

	<!--	Direct rewrites:
				-->
	<target host="blog.fefe.de" />
	<target host="dl.fefe.de" />
	<target host="ptrace.fefe.de" />
	<target host="www.fefe.de" />

	<rule from="^http:"
		to="https:" />
</ruleset>
