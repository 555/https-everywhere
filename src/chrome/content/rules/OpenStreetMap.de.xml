<!--
	For OpenStreetMap.de

		Works:

			- josm

		Doesn't Work:
			- www
			- $
-->
<ruleset name="OpenStreetMap.de">
  <target host="openstreetmap.de" />
  <target host="*.openstreetmap.de" />

  <rule from="^http://josm\.openstreetmap\.de/"
          to="https://josm.openstreetmap.de/" />
</ruleset>