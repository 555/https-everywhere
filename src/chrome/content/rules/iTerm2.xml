<!--
	For iterm2.com

		Works:

			- www
			- $

		Doesn't Work:
			nil

		www redirects to $
		doing rewrite here for performance.
-->
<ruleset name="iTerm2">
  <target host="iterm2.com" />
  <target host="*.iterm2.com" />

  <rule from="^http://(?:www\.)?iterm2\.com/"
          to="https://iterm2.com/" />
</ruleset>